{
  "name": "dnd-lookup",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite-ssg build",
    "serve": "vite preview",
    "check-style": "prettier --check ./index.html ./tests/*.js ./src/**/*.{css,js,vue}",
    "test": "codeceptjs run",
    "server-test": "start-server-and-test dev http://localhost:5173/index.html",
    "netlify": "npx playwright install chromium && npm run server-test && npm run build",
    "lighthouse": "npm run build && \"node_modules/.bin/start-server-and-test\" serve http://localhost:4173/index.html 'lighthouse http://localhost:4173/index.html --view'"
  },
  "dependencies": {
    "vue": "^3.2.19"
  },
  "devDependencies": {
    "@codeceptjs/configure": "^0.8.0",
    "@tailwindcss/typography": "^0.5.2",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vueuse/head": "^0.7.7",
    "autoprefixer": "^10.3.6",
    "codeceptjs": "^3.3.3",
    "critters": "^0.0.16",
    "marked": "^4.0.17",
    "netlify-plugin-cache": "^1.0.3",
    "playwright": "^1.22.2",
    "postcss": "^8.3.8",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.3",
    "vite": "^3.0.0",
    "vite-ssg": "^0.20.2"
  }
}
